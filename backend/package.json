  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean;npm run lint;npm run compile",
    "compile": "npm run routes;tsc --sourceMap false --project tsconfig.json",
    "watch": "tsc -w  --project tsconfig.json",
    "e2e": "LOGGING_GENERAL=Debug mocha -r ts-node/register tests/e2e/**/*.spec.ts --exit",
    "tsoa-all": "npm run swagger; npm run routes",
    "swagger": "tsoa spec",
    "routes": "tsoa routes",
    "start": "node index.js",
    "dev": "LOGGING_GENERAL=Debug nodemon index.js",
    "debug": "LOGGING_GENERAL=Debug nodemon --watch dist --debug --debug-brk=5858 index.js",
    "lint": "tslint --project tsconfig.json --config ../tslint.json"
  },
